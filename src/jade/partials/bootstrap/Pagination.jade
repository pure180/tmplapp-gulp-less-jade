.col-md-12
  h2= page.name

.col-md-6
  nav
    ul.pagination
      li
        a(href='#' aria-label='Previous')
          span(aria-hidden='true') &laquo;
      li
        a(href='#') 1
      li
        a(href='#') 2
      li
        a(href='#') 3
      li
        a(href='#') 4
      li
        a(href='#' aria-label='Next')
          span(aria-hidden='true') &raquo;
.col-md-6        
  nav
    ul.pagination
      li.disabled
        a(href='#' aria-label='Previous')
          span(aria-hidden='true') &laquo;
      li.active
        a(href='#') 1
          span.sr-only (current)
      li
        a(href='#') 2
      li
        a(href='#') 3
      li
        a(href='#') 4
      li
        a(href='#' aria-label='Next')
          span(aria-hidden='true') &raquo;